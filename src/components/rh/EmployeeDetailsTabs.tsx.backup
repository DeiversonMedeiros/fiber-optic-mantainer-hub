            onSubmit={createDependent.mutateAsync}
            onUpdate={updateDependent.mutateAsync}
            onDelete={deleteDependent.mutateAsync}
            loading={dependentsLoading || createDependent.isPending || updateDependent.isPending || deleteDependent.isPending}
          />
        </TabsContent>

        <TabsContent value="benefits" className="mt-6">
          <EmployeeBenefits
            employeeId={employee.id}
            benefits={benefits}
            benefitTypes={benefitTypes}
            onSubmit={createBenefit.mutateAsync}
            onUpdate={updateBenefit.mutateAsync}
            onDelete={deleteBenefit.mutateAsync}
            loading={benefitsLoading || createBenefit.isPending || updateBenefit.isPending || deleteBenefit.isPending}
          />
        </TabsContent>

        <TabsContent value="convenios" className="mt-6">
          <EmployeeConvenios
            employeeId={employee.id}
            companyId={companyId || ''}
          />
        </TabsContent>

        <TabsContent value="vrva" className="mt-6">
          <EmployeeVrVa
            employeeId={employee.id}
            companyId={companyId || ''}
          />
        </TabsContent>

        <TabsContent value="transporte" className="mt-6">
          <EmployeeTransporte
            employeeId={employee.id}
            companyId={companyId || ''}
          />
        </TabsContent>

        <TabsContent value="contracts" className="mt-6">
          <EmployeeContracts
            employeeId={employee.id}
            contracts={contracts}
            positions={positions}
            workShifts={workShifts}
            onSubmit={createContract.mutateAsync}
            onUpdate={updateContract.mutateAsync}
            onDelete={deleteContract.mutateAsync}
            loading={contractsLoading || createContract.isPending || updateContract.isPending || deleteContract.isPending}
          />
        </TabsContent>

        <TabsContent value="tax" className="mt-6">
          <EmployeeFiscal
            employeeId={employee.id}
            employee={employee}
            loading={loading}
          />
        </TabsContent>
      </Tabs>
    </div>
  );
}

